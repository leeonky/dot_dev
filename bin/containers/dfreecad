. $(dirname $0)/../dmg/dmg

IMAGE_CONTAINER=$(basename $0)

DOCKER_REPO=leeonky/
DOCKER_IMAGE=free-cad
IMAGE_VERSION=ubuntu-20
WORKING_USER=gauss
WORKING_DIR='/opt/share/'
VOLUMNS=('/var/run/docker.sock:/var/run/docker.sock')
VOLUMNS+=('host_share:/share')
START_COMMANDS=('tail' '-f' '/dev/null')
WORKING_ENV=("DEV_HOST=$DEV_HOST")
dmg_main "$@"

