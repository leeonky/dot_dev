. $(dirname $0)/../dmg/dmg

IMAGE_CONTAINER=$(basename $0)

DOCKER_REPO=leeonky/
DOCKER_IMAGE=gradle
IMAGE_VERSION=ubuntu
WORKING_USER=gauss
WORKING_DIR='/share/'
WORKING_ADDR="$DEV_HOST:$WORKING_DIR"
# IMAGE_VERSION=1.0.1
VOLUMNS=('/var/run/docker.sock:/var/run/docker.sock' 'gradle_repo:/home/gauss/.gradle/' 'gradle_home:/home/gauss/gradle')
START_COMMANDS=('main_loop')
SHM_SIZE=8G
WORKING_ENV=("DEV_HOST=$DEV_HOST")


dmg_main "$@"
