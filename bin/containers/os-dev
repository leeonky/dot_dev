. $(dirname $0)/../dmg/dmg

IMAGE_CONTAINER=$(basename $0)

DOCKER_REPO=leeonky/
IMAGE_VERSION=ubuntu-20

WORKING_USER=gauss

VOLUMNS=('/var/run/docker.sock:/var/run/docker.sock')
VOLUMNS+=('/opt/share/dot_dev:/home/gauss')
VOLUMNS+=('host_share:/opt/share')
VOLUMNS+=('host_config:/home/gauss/host')
VOLUMNS+=('/tmp/:/tmp/')

START_COMMANDS=('tail' '-f' '/dev/null')

dmg_main "$@"
